<div class="container my-4">
    <div class="row">
        <div class="col-md-12">
            <div class="accordion" id="accordionExample">

                <div class="card">
                    <div class="card-header row" id="headingThree">

                        <div class="col-md-10">
                            <h2 class="mb-0">
                                <button class="btn btn-link btn-block text-left collapsed" type="button"
                                    data-toggle="collapse" data-target="#listReal" aria-expanded="false"
                                    aria-controls="listReal">
                                    Liste des messages
                                </button>

                            </h2>
                        </div>

                    </div>


                    {{!--
                    Tableau---------------------------------------------------------------------------------------------------------------
                    --}}


                    <id id="listReal" class="collapse show" aria-labelledby="listReal" data-parent="#accordionExample">
                        <div class="card-body">
                            <table class="table table-bordered">
                                <thead class="thead-dark">
                                    <tr>
                                        <div class="row text-center ">

                                            <th scope="col" class="text-center">Titre</th>
                                            <th scope="col" class="text-center">Description</th>
                                            <th scope="col" class="text-center">Prix</th>
                                            <th scope="col" class="text-center">Temps passé</th>
                                            <th scope="col" class="text-center">Fichier</th>
                                            <th scope="col" class="text-center">Supprimer</th>

                                        </div>
                                    </tr>
                                </thead>
                                <tbody>
                                    {{!-- Données du Tableau ---------------------------}}
                                    <tr>

                                        <td class="text-center">{{Messages.name}}</td>
                                        <td class="text-center">{{Messages.firstName}}</td>
                                        <td class="text-center">{{Messages.email}}</td>
                                        <td class="text-center">{{Messages.object}}</td>
                                        <td class="text-center">{{Messages.content}}</td>








                                        {{!-- Bouton Supprimer --}}

                                        <td class="text-center">
                                            <button type="button" class="btn btn-danger" data-toggle="modal"
                                                data-target="#deleteItem">
                                                Supprimer
                                            </button>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </id>
                </div>

            </div>
        </div>




    </div>
</div>
</div>
</div>



